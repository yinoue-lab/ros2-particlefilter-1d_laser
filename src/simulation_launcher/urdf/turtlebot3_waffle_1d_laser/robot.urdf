      <origin rpy="0 1.57 0" xyz="-0.025 0 0"/>
      <geometry>
        <cylinder length="0.05" radius="0.01"/>
      </geometry>
    </visual>
    <visual>
      <!-- front leg -->
      <origin rpy="0 0 0" xyz="-0.008 0 -0.015"/>
      <geometry>
        <cylinder length="0.02" radius="0.002"/>
      </geometry>
    </visual>
    <!-- back leg -->
    <visual>
      <origin rpy="0 0 0" xyz="-0.04 0 -0.015"/>
      <geometry>
        <cylinder length="0.02" radius="0.002"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="0.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>
  <gazebo reference="back_laser_link">
    <sensor name="back_laser" type="ray">
      <visualize>true</visualize>
      <always_on>true</always_on>
      <update_rate>30</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1</samples>
            <resolution>1</resolution>
            <min_angle>-0.01</min_angle>
            <max_angle>0.01</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_ray_sensor.so" name="back_laser_plugin">
        <ros>
          <namespace>waffle_1d</namespace>
          <remapping>~/out:=back_laser_range</remapping>
        </ros>
        <output_type>sensor_msgs/Range</output_type>
        <frame_name>back_laser_link</frame_name>
        <radiation_type>infrared</radiation_type>
      </plugin>
    </sensor>
  </gazebo>
  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="turtlebot3_diff_drive">
      <ros>
        <namespace>waffle_1d</namespace>
      </ros>
      <update_rate>30</update_rate>
      <!-- wheels -->
      <left_joint>wheel_left_joint</left_joint>
      <right_joint>wheel_right_joint</right_joint>
      <!-- kinematics -->
      <wheel_separation>0.287</wheel_separation>
      <wheel_diameter>0.066</wheel_diameter>
      <!-- limits -->
      <max_wheel_torque>20</max_wheel_torque>
      <max_wheel_acceleration>1.0</max_wheel_acceleration>
      <command_topic>cmd_vel</command_topic>
      <!-- output -->
      <publish_odom>true</publish_odom>
      <publish_odom_tf>false</publish_odom_tf>
      <publish_wheel_tf>false</publish_wheel_tf>
      <odometry_topic>odom</odometry_topic>
      <odometry_frame>map</odometry_frame>
      <robot_base_frame>base_footprint</robot_base_frame>
    </plugin>
  </gazebo>
  <gazebo>
    <plugin filename="libgazebo_ros_p3d.so" name="gazebo_ros_p3d">
      <ros>
        <namespace>waffle_1d</namespace>
        <remapping>odom:=true_position</remapping>
      </ros>
      <body_name>base_footprint</body_name>
      <frame_name>map</frame_name>
      <update_rate>30</update_rate>
      <xyz_offset>0 0 0</xyz_offset>
      <rpy_offset>0 0 0</rpy_offset>
      <gaussian_noise>0.0</gaussian_noise>
    </plugin>
  </gazebo>
</robot>
